{
  "name": "renderer",
  "version": "1.1.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "http://git.tm.tmcs/ccp-discovery/renderer.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf server/dist || echo 'clean'",
    "clean:dev": "rm -rf server/dist/[nwb][oeu]*",
    "webpack": "env UV_THREADPOOL_SIZE=42 webpack",
    "webpack:prod": "env UV_THREADPOOL_SIZE=42 NODE_ENV=production MINIFY=true USE_FLUENT=true webpack",
    "start": "npm run clean:dev && npm run server:dev",
    "start:dev": "npm run start",
    "start:prod": "npm run build:prod && npm run server:prod",
    "server:dev": "env HMR=true NODE_ENV=development node conf/webpack/run-dev-env.js | bunyan -o short",
    "server:prod": "node server/dist/node/main.js | bunyan -o short",
    "build": "npm run clean && npm run build:dev:server && ",
    "build:graph": "env UV_THREADPOOL_SIZE=42 webpack --json --config conf/webpack/conf.browser.js > server/dist/graph.js",
    "build:dev:server": "npm run webpack -- --config conf/webpack/conf.server.js",
    "build:dev:browser": "npm run webpack -- --config conf/webpack/conf.browser.js",
    "build:prod": "npm run clean && npm run build:prod:server && npm run build:prod:browser",
    "build:prod:server": "npm run webpack:prod -- --config conf/webpack/conf.server.js",
    "build:prod:browser": "npm run webpack:prod -- --config conf/webpack/conf.browser.js",
    "build:docker:base": "docker build -f ./conf/docker/base.dockerfile --tag ccp-discovery/base .",
    "build:docker:predeploy": "docker build -f ./conf/docker/dev.predeploy.dockerfile --tag ccp-discovery/predeploy .",
    "build:docker": "docker run -it -v /var/run/docker.sock:/var/run/docker.sock -v $(pwd):/code ccp-discovery/predeploy",
    "build:changelog": "conf/changelog/build.sh",
    "test": "mocha --opts .mocha",
    "test:watch": "npm test -- --watch",
    "test:coverage": "babel-node ./node_modules/.bin/isparta cover --include '**/*.js*' _mocha -- --opts .mocha",
    "test:coverage:open": "open coverage/lcov-report/index.html",
    "test:acceptance": "nightwatch --config ./conf/test/acceptance_tests/nightwatch.conf.js",
    "test:acceptance:master": "npm run test:acceptance",
    "test:acceptance:all": "npm run test:acceptance -- --env chrome,firefox,phantomjs",
    "test:acceptance:chrome": "npm run test:acceptance -- --env chrome",
    "test:acceptance:firefox": "npm run test:acceptance -- --env firefox",
    "test:acceptance:phantomjs": "npm run test:acceptance -- --env phantomjs",
    "test:acceptance:ios": "npm run test:acceptance -- --env ios",
    "test:acceptance:android": "npm run test:acceptance -- --env s6,nexus7",
    "test:acceptance:s6": "npm run test:acceptance -- --env s6",
    "test:acceptance:nexus7": "npm run test:acceptance -- --env nexus7",
    "test:acceptance:local": "node server/dist/node/main.js > /dev/null & env TEST_URL=\"http://localhost:1338\" nightwatch --config './conf/test/acceptance_tests/nightwatch.conf.js'",
    "test:acceptance:launch": "npm run build && npm run test:acceptance:local --",
    "lint:js": "eslint --fix **/*.js*",
    "lint:css": "stylelint 'frontend/**/*.scss'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bunyan": "1.8.1",
    "fluent-logger-stream": "0.0.1",
    "message-format-inline": "1.0.4",
    "mime-types": "2.1.12",
    "nock": "8.0.0",
    "moment": "2.15.1",
    "react": "15.3.2",
    "serve-static": "1.11.1",
    "sinon": "1.17.6",
    "source-map-support": "0.4.3"
  },
  "devDependencies": {
    "autoprefixer": "6.5.0",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-extract-format-message": "5.0.0",
    "babel-plugin-lodash": "3.2.9",
    "babel-plugin-react-remove-properties": "0.2.4",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-format-message": "5.0.0",
    "babel-plugin-transform-react-display-name": "6.8.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-plugin-transform-strip-svg-sprite": "1.1.0",
    "babel-plugin-typecheck": "3.9.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.16.0",
    "chai": "3.5.0",
