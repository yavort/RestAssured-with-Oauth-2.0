    "strip-loader": "0.1.2",
    "style-loader": "0.13.1",
    "stylelint": "7.3.1",
    "stylelint-config-standard": "13.0.2",
    "stylelint-scss": "1.3.4",
    "superagent": "2.3.0",
    "superagent-promise": "1.1.0",
    "svg-sprite-loader": "0.0.29",
    "ua-parser-js": "0.7.10",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.7",
    "webpack-stats-plugin": "^0.1.3"
  }
}
