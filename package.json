    "conventional-gitlab-releaser": "1.0.14",
    "conventional-recommended-bump": "0.3.0",
    "cors-anywhere": "0.4.0",
    "css-loader": "0.25.0",
    "enzyme": "2.4.1",
    "eslint": "3.7.1",
    "eslint-config-standard": "6.2.0",
    "eslint-config-standard-react": "4.2.0",
    "eslint-plugin-import": "2.0.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-react": "6.3.0",
    "eslint-plugin-standard": "2.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "fbjs": "0.8.5",
    "fbjs-scripts": "0.7.1",
    "file-loader": "0.9.0",
    "format-message": "5.0.0",
    "google-map-react": "0.20.0",
    "hoist-non-react-statics": "1.2.0",
    "ignore-styles": "5.0.1",
    "isomorphic-style-loader": "1.0.0",
    "isparta": "4.0.0",
    "jsdom": "9.6.0",
    "json": "9.0.4",
    "json5-loader": "0.6.0",
    "kefir": "3.5.1",
    "lodash": "4.16.4",
    "lodash-webpack-plugin": "0.10.2",
    "lost": "7.1.0",
    "mocha": "3.1.0",
    "nightwatch": "0.9.8",
    "node-sass": "3.10.1",
    "normalize-paths": "1.1.8",
    "path-to-regexp": "1.6.0",
    "postcss-loader": "0.13.0",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-cookie": "0.4.8",
    "react-dom": "15.3.2",
    "react-overlays": "0.6.10",
    "redux": "3.6.0",
